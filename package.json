{
  "name": "vue-ssr-boilerplate",
  "version": "1.0.0",
  "description": "Lite Vue Frontend boilerplate with SSR enabled.",
  "scripts": {
    "dev": "node server",
    "start": "cross-env NODE_ENV=production node server",
    "build": "rimraf dist && npm run build:client && npm run build:server",
    "lint": "eslint --ext .js,.vue src",
    "build:client": "cross-env NODE_ENV=production webpack --mode production --config build/webpack.client.config.js --progress",
    "build:server": "cross-env NODE_ENV=production webpack --mode production --config build/webpack.server.config.js --progress"
  },
  "engines": {
    "node": ">=6"
  },
  "author": "Adrien Beaudouin",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.4",
    "@fortawesome/free-brands-svg-icons": "^5.3.1",
    "@fortawesome/free-regular-svg-icons": "^5.3.1",
    "@fortawesome/free-solid-svg-icons": "^5.3.1",
    "@fortawesome/vue-fontawesome": "^0.1.1",
    "autoprefixer": "^9.1.5",
    "babel-eslint": "^9.0.0",
    "babel-loader": "^8.0.2",
    "bootstrap": "^4.1.3",
    "css-loader": "^1.0.0",
    "eslint": "^5.6.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^5.0.0-beta.3",
    "file-loader": "^2.0.0",
    "font-awesome": "^4.7.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "jquery": "^3.3.1",
    "memory-fs": "^0.4.1",
    "mini-css-extract-plugin": "^0.4.3",
    "node-sass": "^4.9.3",
    "noty": "^3.2.0-beta",
    "popper.js": "^1.14.4",
    "postcss-loader": "^3.0.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.0",
    "sweetalert2": "^7.28.2",
    "url-loader": "^1.1.1",
