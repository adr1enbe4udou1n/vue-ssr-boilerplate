    "webpack-node-externals": "^1.7.2",
    "webpack-notifier": "^1.6.0"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "bootstrap-vue": "^2.0.0-rc.11",
    "cross-env": "^5.2.0",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "lru-cache": "^4.1.3",
    "vue": "^2.5.17",
    "vue-i18n": "^8.1.0",
    "vue-meta": "^1.5.4",
    "vue-notifications": "^0.9.0",
